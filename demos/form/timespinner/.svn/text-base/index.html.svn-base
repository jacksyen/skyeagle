		<link rel="stylesheet" href="../../js/hl/style.css" />
		
		<link rel="stylesheet" href="../../css/command.css" />
		<link rel="stylesheet" href="../../css/index.css" />
		<script>
			$(document).ready(function() {
				/**
				 * demo 1 
				 */
				$('#timespinner1').timespinner({
					Separator : ':'	// 分隔符
				});
				/**
				 * demo 2 
				 */
				$('#timespinner2').timespinner({
					Separator : ':'	// 分隔符
				});
				$('#disable').click(function (){
					$('#timespinner2').timespinner('disable');
				});
				$('#enable').click(function (){
					$('#timespinner2').timespinner('enable');
				});
				
				// 代码高亮
				DlHighlight.HELPERS.highlightByName("timespinner", "pre");
			});
		</script>
		<style>
			
		</style>
	</head>
	<body>
		<div class="body">
			<div class="top">

			</div>
			<div class="main">
				<div class="leftColumn">
					<!-- init command -->
				</div>
				<div class="mainColumn">
					<div class="block" id="demos-nav">
						<h4>Examples</h4>
						<ul>
							<li class="demo-config-on"><a href="#demo1">默认功能</a></li>
							<li><a href="#demo2">方法示例</a></li>
							<!-- <li><a href="#demo3">显示按钮</a></li> -->
						</ul>
					</div>
					<div class="block" id="demo1">
						<h1>示例一（默认功能）</h1>
						<div class="line-block" >
								<input type="text" id="timespinner1" />
							<div class="code">
<pre name="timespinner" class="js">
$('#timespinner1').timespinner({
	Separator : ':'	// 分隔符
});
</pre>
							</div>
						</div>
					</div>
					<div class="block" id="demo2">
						<h1>示例二（方法示例）</h1>
						<div class="line-block" >
								<input type="text" id="timespinner2" />
							<div class="code">
<button id="disable">disable</button>
<button id="enable">enable</button>								
<pre name="timespinner" class="js">
$('#timespinner2').timespinner({
	Separator : ':'	// 分隔符
});
//禁用时间微调器
$('#disable').click(function (){
	$('#timespinner2').timespinner('disable');
});
//启用时间微调器
$('#enable').click(function (){
	$('#timespinner2').timespinner('enable');
});
</pre>
							</div>
						</div>
					</div>
					<!-- end of demo1 -->
				</div>
			</div>
		</div>
	</body>
