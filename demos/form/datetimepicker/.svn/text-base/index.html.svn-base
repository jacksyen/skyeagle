					<div class="block" id="demo1">
						<h1>示例一（默认功能）</h1>
						<div class="line-block" >
								Date:
								<input type="text" id="datetimepicker1">
							<div class="code">
<pre name="datetimepicker" class="js">
$('#datetimepicker1').datetimepicker({
	
});
</pre>
							</div>
						</div>
					</div>
					<div class="block" id="demo2">
						<h1>示例二（方法示例）</h1>
						<div class="line-block" >
								Date:
								<input type="text" id="datetimepicker2">
							<div class="code">
<button id="destroy">destroy</button>
<button id="disable">disable</button>
<button id="enable">enable</button>
<button id="show">show</button>
<!-- <button id="getDate">getDate</button>
<button id="setDate">setDate</button>	 -->							
<pre name="datetimepicker" class="js">
$('#datetimepicker2').datetimepicker({
	
});
$('#destroy').click(function (){
	$('#datepicker2').datetimepicker( "destroy" );
});
$('#disable').click(function (){
	$('#datepicker2').datetimepicker( "disable" );
});
$('#enable').click(function (){
	$('#datepicker2').datetimepicker( "enable" );
});
$('#show').click(function (){
	$('#datepicker2').datetimepicker( "show" );
});
</pre>
							</div>
						</div>
					</div>
					<div class="block" id="demo3">
						<h1>示例三（日期示例）</h1>
						<div class="line-block" >
								Date:
								<input type="text" id="datetimepicker3">
							<div class="code">
<pre name="datetimepicker" class="js">
$('#datetimepicker3').datetimepicker({
	dateFormat : 'dd/mm/yy',	// 按照英文格式显示日期
	defaultDate : '+1m+7d'		// 初始显示的日期（距今天一个月加七天之后）
});
</pre>
							</div>
						</div>
					</div>
					<div class="block" id="demo3">
						<h1>示例四（显示示例）</h1>
						<div class="line-block" >
								Date:
								<input type="text" id="datetimepicker4">
							<div class="code">
<pre name="datetimepicker" class="js">
$('#datetimepicker4').datetimepicker({
	numberOfMonths : 2,		// 设置每次显示几个月
	firstDay : 1			// 设置每周的第一天
});
</pre>
							</div>
						</div>
					</div>
					<!-- end of demo1 -->
				</div>
