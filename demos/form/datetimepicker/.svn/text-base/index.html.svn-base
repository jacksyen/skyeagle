		<script>
			$(document).ready(function() {
				/**
				 * demo 1 
				 */
				$('#datetimepicker1').datetimepicker({
					
				});
				/**
				 * demo 2 
				 */
				$('#datetimepicker2').datetimepicker({
					
				});
				$('#destroy').click(function (){
					$('#datetimepicker2').datetimepicker( "destroy" );
				});
				$('#disable').click(function (){
					$('#datetimepicker2').datetimepicker( "disable" );
				});
				$('#enable').click(function (){
					$('#datetimepicker2').datetimepicker( "enable" );
				});
				$('#show').click(function (){
					$('#datetimepicker2').datetimepicker( "show" );
				});
				// $('#getDate').click(function (){
					// var date = $('#datetimepicker2').datepicker( "getDate" );
					// alert('getDate:' + date);
				// });
				// $('#setDate').click(function (){
					// $('#datetimepicker2').datepicker( "setDate", "2012-09-30" );
				// });
				/**
				 * demo 3 
				 */
				$('#datetimepicker3').datetimepicker({
					dateFormat : 'dd/mm/yy',
					defaultDate : '+1m+7d'
				});
				/**
				 * demo 4 
				 */
				$('#datetimepicker4').datetimepicker({
					numberOfMonths : 2,		// 设置每次显示几个月
					firstDay : 1			// 设置每周的第一天
				});
				
				
				// 代码高亮
				DlHighlight.HELPERS.highlightByName("datetimepicker", "pre");
			});
		</script>
