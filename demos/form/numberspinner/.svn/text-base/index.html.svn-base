		<script type="text/javascript" language="JavaScript" src="../../../plugins/form/jquery.ui.validatebox.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../../plugins/form/jquery.ui.numberbox.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../../plugins/form/jquery.ui.numberspinner.js"></script>

		<script type="text/javascript" language="JavaScript" src="../../js/left.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../js/hl/highlight.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../js/hl/lang-js.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../js/hl/lang-xml.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../js/hl/lang-html.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../js/hl/lang-css.js"></script>
		<script type="text/javascript" language="JavaScript" src="../../js/hl/helpers.js"></script>
		<!-- 		<script type="text/javascript" language="JavaScript" src="../../js/main.js"></script> -->

		
		<link rel="stylesheet" href="../../../skins/jquery.ui.core.css" />
		<link rel="stylesheet" href="../../../skins/jquery.ui.theme.css" />
		<link rel="stylesheet" href="../../../skins/jquery.ui.validatebox.css" />
		<link rel="stylesheet" href="../../../skins/jquery.ui.numberspinner.css" />

		<link rel="stylesheet" href="../../js/hl/style.css" />
		
		<link rel="stylesheet" href="../../css/command.css" />
		<link rel="stylesheet" href="../../css/index.css" />
		<script>
			$(document).ready(function() {
				/**
				 * demo 1 
				 */
				$('#numberspinner1').numberspinner({
					// 设置初始值
					initnumber : null,
					// 增量
					addnumber  : 2,
					// 精度
					precision  : 2,		
					// 是否需要处理精度
					parseable  : true,
					// 设置宽度
					width 	   : 300
				});
				/**
				 * demo 2 
				 */
				$('#numberspinner2').numberspinner({
					// 设置初始值
					initnumber : 10,
					// 增量
					addnumber  : 2,
					// 精度
					precision  : 2,		
					// 是否需要处理精度
					parseable  : true,
					// 设置宽度
					width 	   : 300
				});
				$('#disable').click(function (){
					$('#numberspinner2').numberspinner('disable');
				});
				$('#enable').click(function (){
					$('#numberspinner2').numberspinner('enable');
				});
				
				
				// 代码高亮
				DlHighlight.HELPERS.highlightByName("numberspinner", "pre");
			});
		</script>
		<style>
			
		</style>
	</head>
	<body>
		<div class="body">
			<div class="top">

			</div>
			<div class="main">
				<div class="leftColumn">
					<!-- init command -->
				</div>
				<div class="mainColumn">
					<div class="block" id="demos-nav">
						<h4>Examples</h4>
						<ul>
							<li class="demo-config-on"><a href="#demo1">默认功能</a></li>
							<li><a href="#demo2">方法示例</a></li>
							<!--<li><a href="#demo3">显示按钮</a></li> -->
						</ul>
					</div>
					<div class="block" id="demo1">
						<h1>示例一（默认功能）</h1>
						<div class="line-block" >
								<input type="text" id="numberspinner1" />
							<div class="code">
<pre name="numberspinner" class="js">
$('#numberspinner1').numberspinner({
	initnumber : null,	// 设置初始值
	addnumber  : 2,	// 增量
	parseable  : true,	// 是否需要处理精度
	precision  : 2		// 精度
});
</pre>
							</div>
						</div>
					</div>
					<div class="block" id="demo2">
						<h1>示例二（方法示例）</h1>
						<div class="line-block" >
								<input type="text" id="numberspinner2" />
							<div class="code">
<button id="disable">disable</button>
<button id="enable">enable</button>										
<pre name="numberspinner" class="js">
$('#numberspinner2').numberspinner({
	initnumber : 10,	// 设置初始值
	addnumber  : 2,	// 增量
	parseable  : true,	// 是否需要处理精度
	precision  : 2		// 精度
});
//禁用数字微调器
$('#disable').click(function (){
	$('#numberspinner2').numberspinner('disable');
});
//启用数字微调器
$('#enable').click(function (){
	$('#numberspinner2').numberspinner('enable');
});
</pre>
							</div>
						</div>
					</div>
					<!-- end of demo1 -->
				</div>
			</div>
		</div>
	</body>
</html>