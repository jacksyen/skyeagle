		<script>
			$(document).ready(function() {
				/**
				 * demo 1 
				 */
				$('#combogrid1').combogrid({
					// combo
					width : 300,
					panelWidth : 600,
					// datagrid
					pagination : true,
					pageSize : 10,
					pageList : [5,10],
					rownumbers: true,
					singleSelect : true,
					sortName : 'code',
					sortOrder : 'desc',
					url : 'datagrid_data.json',
					columns:[[{
							title : '合并列',
							colspan : 2
						},{
							field:'code',
							title:'Code',
							width:80,
							rowspan: 2,
							editor : {type: 'numberbox' }
						}
					],[
						{
							field:'name',
							title:'Name',
							width:120,
							editor : {type: 'combo' }
						},
						{
							field:'addr',
							title:'Addr',
							width:80,
							align:'right',
							sortable : true,
							sorter : function(a, b) {
								return (a > b ? 1 : -1);
							}
						}
					]]
				});
				/**
				 * demo 2
				 */
				$('#combogrid2').combogrid({
					// combo
					width : 300,
					panelWidth : 600,
					// datagrid
					pagination : true,
					pageSize : 10,
					pageList : [5,10],
					rownumbers: true,
					singleSelect : true,
					sortName : 'code',
					sortOrder : 'desc',
					url : 'datagrid_data.json',
					columns:[[{
							title : '合并列',
							colspan : 2
						},{
							field:'code',
							title:'Code',
							width:80,
							rowspan: 2,
							editor : {type: 'numberbox' }
						}
					],[
						{
							field:'name',
							title:'Name',
							width:120,
							editor : {type: 'combo' }
						},
						{
							field:'addr',
							title:'Addr',
							width:80,
							align:'right',
							sortable : true,
							sorter : function(a, b) {
								return (a > b ? 1 : -1);
							}
						}
					]]
				});
				$('#destroy').click(function (){
					$('#combogrid2').combogrid('destroy');
				});
				$('#resize').click(function (){
					$('#combogrid2').combogrid('resize',{
						width : 300
					});
				});
				$('#disable').click(function (){
					$('#combogrid2').combogrid('disable');
				});
				$('#enable').click(function (){
					$('#combogrid2').combogrid('enable');
				});
				$('#getValue').click(function (){
					var val = $('#combogrid2').combogrid('getValue');
					alert('getVlaue :' + val);
				});
				$('#setValue').click(function (){
					var val = $('#combogrid2').combogrid('setValue','File1');
				});
				
				// 代码高亮
				DlHighlight.HELPERS.highlightByName("combogrid", "pre");
			});
		</script>
