					<div class="block" id="demo2">
						<h1>示例三（方法示例）</h1>
						<div class="line-block" >
							<div id="combogrid2">
								
							</div>
							<div class="code">
<button id="destroy">destroy</button>
<button id="resize">resize</button>
<button id="disable">disable</button>
<button id="enable">enable</button>
<button id="getValue">getValue</button>	
<button id="setValue">setValue</button>							
<pre name="combogrid" class="js">
$('#combogrid2').combogrid({
	width : 300,	// combo的宽度
	panelWidth : 600,	// panel和grid的宽度
	pagination : true,	// 是否分页
	pageSize : 10,		// 每页条数
	pageList : [5,10],	
	rownumbers: true,
	singleSelect : true,
	sortName : 'code',
	sortOrder : 'desc',
	url : 'datagrid_data.json',		// 传入的json数据
	columns:[[{
			title : '合并列',
			colspan : 2
		},{
			field:'code',
			title:'Code',
			width:80,
			rowspan: 2,
			editor : {type: 'numberbox' }
		}
	],[
		{
			field:'name',
			title:'Name',
			width:120,
			editor : {type: 'combo' }
		},
		{
			field:'addr',
			title:'Addr',
			width:80,
			align:'right',
			sortable : true,
			sorter : function(a, b) {
				return (a > b ? 1 : -1);
			}
		}
	]]
});
$('#destroy').click(function (){
	$('#combogrid2').combogrid('destroy');
});
$('#resize').click(function (){
	$('#combogrid2').combogrid('resize',{
		width : 300
	});
});
$('#disable').click(function (){
	$('#combogrid2').combogrid('disable');
});
$('#enable').click(function (){
	$('#combogrid2').combogrid('enable');
});
$('#getValue').click(function (){
	var val = $('#combogrid2').combogrid('getValue');
	alert('getVlaue :' + val);
});
$('#setValue').click(function (){
	var val = $('#combogrid2').combogrid('setValue','Name 1');
});
</pre>
