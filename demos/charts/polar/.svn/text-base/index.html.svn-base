<pre name="polarchart" class="js">
new Highcharts.Chart({
	// chart主体配置
	chart : {
		// 在哪个DOM
		renderTo : 'demo1Container'
	},
	// 主标题
	title : {
		text : '基础横向chart',
		x : 0
	},
	// 副标题
	subtitle : {
		text : '副标题',
		x : 0
	},
	// 其他配置
	plotOptions : {
		// 饼图配置
		pie : {
			allowPointSelect : true,
			cursor : 'pointer',
			dataLabels : {
				enabled : true
			}
		}
	},
	// 数据
	series: [{
        type: 'pie',
        name: '浏览器占有率',
        data: [
            ['Firefox',   45.0],
            ['IE',       26.8],
            {
                name: 'Chrome',
                y: 12.8,
                // 切片
                sliced: true,
				// 是否选择
                selected: true
            },
            ['Safari',    8.5],
            ['Opera',     6.2],
            ['Others',   0.7]
        ]
    }]
});
</pre>
