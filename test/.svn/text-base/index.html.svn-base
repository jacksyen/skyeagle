		<style>
			body {
				width: 100%;
				height: 600px;
			}
			.testContent {
				height : 350px;
			}
		</style>
		<script>
			$(document).ready(function() {
				// layout布局
				var element = $('#page').layout({
					fit : true,
					
					panels : [{
						id : "north-panel",
						title : "This is north panel",
						region : "north",
						resizable : true,
						collapsible : true,
						closable : true,
						height : 50
					}, {
						id : "south-panel",
						title : "This is south panel",
						region : "south",
						resizable : true,
						collapsible : true,
						height : 50
					}, {
						id : "center-panel",
						header : false,
						region : "center"
					}, {
						id : "west-panel",
						resizable : true,
						collapsible : true,
						title : "This is west panel",
						region : "west",
						width : 250
					}, {
						id : "east-panel",
						resizable : true,
						collapsible : true,
						title : "This is east panel",
						region : "east",
						width : 150
					}]
				});
				// center中间添加tabs
				$("#tabs").tabs({
					//disabled: [1, 2],
					collapsible : false,
					closable : true
				});
				// west panel 中添加树
				$('#tt').tree({
					url : 'test_data.json',
					onClick : function(event, ui) {
						switch(ui.text) {
							case "autocomplete" : 
								var s = {
									total : 9,
									data : [
										'中国',
										'密码',
										'是中。。。',
										'dddd',
										'eeee',
										'f#ff',
										'asdxzcv',
										'abaasl',
										'a!asd'
									]
								};
								var autocom = $('<div id="autocomplete"></div>').autocomplete({
									data : s
								});
								autocom.autocomplete('autocomplete').appendTo('.testContent');
								break;
							case "calendar" : 
								var cal = $('<div id="calendar"></div>').datepicker({
								
								});
								cal.appendTo('.testContent');
								break;
							case "combo" : 
								var combo = $('<div id="combo"><div class="select">Select a language</div><div class="sp">Java</div><div class="sp">C#</div><div class="sp">Ruby</div><div class="sp">Basic</div><div class="sp">Fortran</div></div>').combo({
									width : 150,
									panelWidth : 150,
									editable : true,
									radioable : false,
									multiable : true,
									required : true,
									value : 'Java'
								});
								combo.combo('combo').appendTo('.testContent');
								break;
							case "combogrid" :
								var combog = $('<div id="combogrid"></div>').combogrid({
									// combogrid
									idField : 'name',
									// combo
									width : 300,
									panelWidth : 600,
									// datagrid
									pagination : true,
									pageSize : 10,
									pageList : [5,10],
									rownumbers: true,
									singleSelect : true,
									sortName : 'code',
									sortOrder : 'desc',
									url : 'test_datagrid_data.json',
									columns:[[{
											title : '合并列',
											colspan : 2
										},{
											field:'code',
											title:'Code',
											width:80,
											rowspan: 2,
											editor : {type: 'numberbox' }
										}
									],[
										{
											field:'name',
											title:'Name',
											width:120,
											editor : {type: 'combo' }
										},
										{
											field:'addr',
											title:'Addr',
											width:80,
											align:'right',
											sortable : true,
											sorter : function(a, b) {
												return (a > b ? 1 : -1);
											}
										}
									]]
								});
								combog.combo('combo').appendTo('.testContent');
								break;
							case "combotree" : 
								var combot = $('<div id="combotree"></div>').combotree({
									// combo
									width : 200,
									panelWidth : 200,
									// tree   checkbox关系到combotree能否多选
									url : 'test_tree_data.json',
									animate : true,
									checkbox : true,
									cascadeCheck : true
								});
								combot.combo('combo').appendTo('.testContent');
								break;
							case "datepicker" : 
								var datep = $('<input type="text" id="datepicker">').datepicker({
									
								});
								datep.appendTo('.testContent');
								break;
							case "datetimepicker" :
								var datetp = $('<input type="text" id="datetimepicker">').datetimepicker({
									
								});
								datetp.appendTo('.testContent');
								break;
							case "form" :
								var form = $('<form id="myform"><input type="text" id="validatebox" /><br/><input type="submit" /></form>');
								form.find('#validatebox').validatebox({
									required : true
								});
								form.form({
									url : 'xxx.action',
									onSubmit : function (){
										return $(this).form('validate');
									},
									onSuccess : function (ui, data){
										alert('success' + data);
									}
								});
								form.appendTo('.testContent');
								break;
							case "linkagebox" :
								var json = {
									count : 4,	// 级联的层级
									data : [
										['美国', '中国', '英国'],			// data[0]
										{								// data[1]
											'美国' : ['华盛顿', '洛杉矶'],		
											'中国' : ['安徽', '海南'],
											'英国' : ['伦敦', '利物浦']
										},
										{								// data[2]
											'华盛顿' : ['hua1', 'hua2'],
											'洛杉矶' : ['lo1', 'lo2'],
											'安徽' : ['合肥', '安庆'],
											'海南' : ['三亚', '东方'],
											'伦敦' : ['lun1', 'lun2'],
											'利物浦' : ['li1', 'li2']
										},
										{
											'合肥' : ['肥东', '肥西', '庐江', '长丰'],
											'安庆' : ['望江', '太湖', '宿松', '潜山']
										}
									]
								};
								var link = $('<div id="linkagebox1">国家：<span></span>省份：<span></span>市区：<span></span>地区：<span></span></div>').linkagebox({
									data : json
								});
								link.linkagebox('linkagebox').appendTo('.testContent');
								break;
							case "numberbox" :
								var numb = $('<input type="text" id="numberbox" />').numberbox({
									width : 500,
									required : true,
									max : 100,
									min : 5
								});
								numb.numberbox('numberbox').appendTo('.testContent');
								break;
							case "numberspinner" :
								var numsp = $('<input type="text" id="numberspinner" />').numberspinner({
									initnumber : 10,
									addnumber  : 5,
									parseable  : true,
									precision : 2,
									width : 300
								});
								numsp.numberspinner('numberspinner').appendTo('.testContent');
								break;
							case "rating" :
								var rat = $('<div id="rating"></div>').ratingbox({
									
								});
								rat.appendTo('.testContent');
								break;
							case "slider" :
								var sd = $('<div id="slider" style="margin-left: 200px; margin-right: 200px;"></div>').slider({
									
								});
								sd.appendTo('.testContent');
								break;
							case "timespinner" :
								var timesp = $('<input type="text" id="timespinner" />').timespinner({
									width : 500
								});
								timesp.timespinner('timespinner').appendTo('.testContent');
								break;
							case "validatebox" :
								var valbox = $('<input type="text" id="validatebox" />').validatebox({
									required : true,
									width    : 400
								});
								valbox.appendTo('.testContent');
								break;
							case "panel" :
								var panel = $('<div id="panel"></div>').panel({
									width : 300,
									height: 150,
									title : 'panel',
									draggabled : true,
									closable : true
								});
								panel.panel('panel').appendTo('.testContent');
								break;
							case 'treeTab' :
								alert(ui.attributes);
								// $.ajax({
					        		// url : '../demos/menu-button/button/button.back.html',
					        		// async : false,
					        		// success : function (data){
					        			// $('.testContent').html(data);
					        		// }
					        	// });
					        	
								//$('.testContent').load('../demos/menu-button/button/button.back.html')
								$("#tabs").tabs('add',{
									title : 'mmm',
									// content : 'xxxxxx'
									url : ui.attributes
								});
								break;
							default :
								alert('default');
						}
					}
				});
				
			});
		</script>
	</head>
	<body>
		<div id="page">

			<div id="north-panel">
				north
			</div>

			<div id="center-panel">
				<div id="tabs">
					<ul>
						<li>
							<a href="#tabs-1">test tab</a>
						</li>
					</ul>
					<div id="tabs-1">
						<div class="testContent" style="margin-top: 50px; margin-left: 50px;"></div>
					</div>
				</div>
			</div>

			<div id="east-panel">
				east
			</div>

			<div id="west-panel">
				<ul id="tt"></ul>
			</div>

			<div id="south-panel">
				sorth
			</div>
		</div>

	</body>
